<Window x:Class="Materiality.MainDemo.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Materiality.MainDemo"
        xmlns:materiality11="clr-namespace:Materiality.WPF.Controls;assembly=Materiality.WPF"
        mc:Ignorable="d"
        Title="Window1" Height="300" Width="300">
    <Window.Resources>
        <FontFamily x:Key="Icons">pack://application:,,,/Fonts/Icons/#materialdesignicons</FontFamily>
        
    </Window.Resources>
    <Grid>
        <Button x:Name="btn" FontFamily="{StaticResource Icons}" Width="100" Height="100">
        </Button>
    </Grid>
</Window>
